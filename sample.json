{
  "builders": [{
    "type": "file",
    "target": ""
  }],
 "provisioners": [
    {
      "type": "ansible",
      "playbook_file": "./playbook.yml"
    }
  ],
  "post-processors": [
    [
      {
        "type": "docker-tag",
        "repository": "example/repo",
        "tag": "latest",
        "force": "true"
      },
      {
        "type": "docker-push",
        "login": "true",
        "login_username": "login",
        "login_password": "password",
        "login_email": "email"
      }
    ]
  ]
}
