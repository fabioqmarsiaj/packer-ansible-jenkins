pipeline {
    agent {
        docker { image 'fabioqmarsiaj/cloudnative:latest' }
    }
    stages {
        stage('Run docker image') {
            steps {
                sh('cd /usr/local/desafio-final/')
                sh('docker run -p 8090:8081 fabioqmarsiaj/cloudnative:latest')
            }
        }
    }
}