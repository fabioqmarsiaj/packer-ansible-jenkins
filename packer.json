{
  "builders": [{
    "type": "file",
    "target": "./app.war"
  },
  {
    "type": "docker",
    "image": "openjdk8-jre",
    "commit": "true"
  }],
 "provisioners": [
    {
      "type": "ansible",
      "playbook_file": "./playbook.yml"
    }
  ],
  "post-processors": [
    [
      {
        "type": "docker-tag",
        "repository": "example/repo",
        "tag": "latest",
        "force": "true"
      }
    ]
  ]
}
